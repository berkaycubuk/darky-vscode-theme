{
  "name": "darky",
  "type": "dark",
  "colors": {
    "editor.background": "#0f0f0f",
    "editor.foreground": "#eeeeee",
    "activityBarBadge.background": "#a30101",
    "sideBarTitle.foreground": "#d6d6d6",
    "sideBar.background": "#121212",
    "activityBar.background": "#0f0f0f",
    "activityBar.activeBackground": "#5e000071",
    "activityBar.activeBorder": "#5e0000",
    "titleBar.activeBackground": "#0f0f0f",
    "titleBar.inactiveBackground": "#121212",
    "statusBar.debuggingBackground": "#bd0000",
    "button.background": "#5e0000",
    "button.foreground": "#eeeeee",
    "button.hoverBackground": "#790f0f",
    "input.border": "#5e0000",
    "list.activeSelectionBackground": "#5e0000",
    "list.dropBackground": "#5e000015",
    "list.hoverBackground": "#5e000071",
    "textLink.foreground": "#d10909",
    "textLink.activeForeground": "#e22a2a",
    "menu.background": "#121212",
    "menubar.selectionBackground": "#5e0000",
    "tab.unfocusedHoverBackground": "#5e000071",
    "tab.inactiveBackground": "#181818",
    "tab.activeForeground": "#eeeeee",
    "tab.activeBackground": "#0f0f0f",
    "scrollbarSlider.background": "#5e000071",
    "scrollbarSlider.hoverBackground": "#5e0000",
    "scrollbarSlider.activeBackground": "#5e0000",
    "editor.selectionBackground": "#eeeeee1e",
    "editorGroupHeader.tabsBackground": "#181818",
    "statusBar.background": "#5e0000"
